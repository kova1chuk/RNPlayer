    "eslint": "^8.34.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "jest": "^26.6.3",
    "jest-expo": "~47.0.1",
    "lint-staged": "^13.1.2",
    "prettier": "^2.8.4",
    "react-native-svg-transformer": "^1.0.0",
    "react-test-renderer": "18.1.0",
