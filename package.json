    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-native": "0.70.5",
    "react-native-safe-area-context": "4.4.1",
    "react-native-screens": "~3.18.0",
    "react-native-svg": "^13.8.0",
    "react-native-web": "~0.18.9",
    "react-redux": "^8.0.5",
    "redux": "^4.2.1",
